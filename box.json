{
  "main": "bin/chip",
  "banner": [
    "This file is part of the Chip project.",
    "",
    "(c) phith0n <root@leavesongs.com>",
    "https://github.com/phith0n/chip",
    "",
    "This source file is subject to the MIT license that is bundled",
    "with this source code in the file LICENSE."
  ],
  "directories": [
    "src/"
  ],
  "finder": [
    {
      "notName": "/LICENSE|.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock|CHANGELOG|.*\\.rst/",
      "exclude": [
        "doc",
        "test",
        "test_old",
        "tests",
        "Tests",
        "docs",
        "documentation",
        "vendor-bin",
        "CHANGELOG"
      ],
      "in": "vendor/"
    }
  ],
  "dump-autoload": true,
  "exclude-dev-files": false,
  "compression": "GZ"
}